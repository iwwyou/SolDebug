  {
    "code": "    function _insertLockedStake(address _stakerAddress, uint _ATIDamount, uint _stakeWeight, uint _lockedUntil) internal returns (uint newLockedStakeID) {\n}",
    "startLine": 22,
    "endLine": 23,
    "event": "add"
  },
  {
    "code": "        if (nextLockedStakeIDMap[_stakerAddress] == 0) {\n}",
