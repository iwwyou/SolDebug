[
  {
    "code": "contract QANX {\n}",
    "startLine": 1,
    "endLine": 2,
    "event": "add"
  },
  {
    "code": "\n",
    "startLine": 2,
    "endLine": 2,
    "event": "add"
  },
  {
    "code": "    struct Lock {\n}",
    "startLine": 3,
    "endLine": 4,
    "event": "add"
  },
  {
    "code": "        uint256 tokenAmount;   ",
    "startLine": 4,
    "endLine": 4,
    "event": "add"
  },
  {
    "code": "        uint32 hardLockUntil;  ",
    "startLine": 5,
    "endLine": 5,
    "event": "add"
  },
  {
    "code": "        uint32 softLockUntil;   ",
    "startLine": 6,
    "endLine": 6,
    "event": "add"
  },
  {
    "code": "        uint8 allowedHops;      ",
    "startLine": 7,
    "endLine": 7,
    "event": "add"
  },
  {
    "code": "        uint32 lastUnlock;      ",
    "startLine": 8,
    "endLine": 8,
    "event": "add"
  },
  {
    "code": "        uint256 unlockPerSec;  ",
    "startLine": 9,
    "endLine": 9,
    "event": "add"
  },
  {
    "code": "\n",
    "startLine": 11,
    "endLine": 11,
    "event": "add"
  },
  {
    "code": "    mapping (address => Lock) private _locks;",
    "startLine": 12,
    "endLine": 12,
    "event": "add"
  },
  {
    "code": "\n",
    "startLine": 13,
    "endLine": 13,
    "event": "add"
  },
  {
    "code": "    function unlockableBalanceOf(address account) public view virtual returns (uint256) {        \n}",
    "startLine": 14,
    "endLine": 15,
    "event": "add"
  },
  {
    "code": "        if(block.timestamp < _locks[account].hardLockUntil) {\n}",
    "startLine": 15,
    "endLine": 16,
    "event": "add"
  },
  {
    "code": "            return 0;",
    "startLine": 16,
    "endLine": 16,
    "event": "add"
  },
  {
    "code": "\n",
    "startLine": 18,
    "endLine": 18,
    "event": "add"
  },
  {
    "code": "        if(block.timestamp > _locks[account].softLockUntil) {\n}",
    "startLine": 19,
    "endLine": 20,
    "event": "add"
  },
  {
    "code": "            return _locks[account].tokenAmount;",
    "startLine": 20,
    "endLine": 20,
    "event": "add"
  },
  {
    "code": "\n",
    "startLine": 22,
    "endLine": 22,
    "event": "add"
  },
  {
    "code": "        return (block.timestamp - _locks[account].lastUnlock) * _locks[account].unlockPerSec;",
    "startLine": 23,
    "endLine": 23,
    "event": "add"
  },
  {
    "code": "\n",
    "startLine": 25,
    "endLine": 25,
    "event": "add"
  },
  {
    "code": "    function unlock(address account) external returns (bool) {        \n}",
    "startLine": 26,
    "endLine": 27,
    "event": "add"
  },
  {
    "code": "        uint256 unlockable = unlockableBalanceOf(account);",
    "startLine": 27,
    "endLine": 27,
    "event": "add"
  },
  {
    "code": "\n",
    "startLine": 28,
    "endLine": 28,
    "event": "add"
  },
  {
    "code": "        require(unlockable > 0 && _locks[account].tokenAmount > 0 && block.timestamp > _locks[account].hardLockUntil, \"No unlockable tokens!\");",
    "startLine": 29,
    "endLine": 29,
    "event": "add"
  },
  {
    "code": "\n",
    "startLine": 30,
    "endLine": 30,
    "event": "add"
  },
  {
    "code": "        _locks[account].lastUnlock = uint32(block.timestamp);",
    "startLine": 31,
    "endLine": 31,
    "event": "add"
  },
  {
    "code": "        _locks[account].tokenAmount = _locks[account].tokenAmount - unlockable;",
    "startLine": 32,
    "endLine": 32,
    "event": "add"
  },
  {
    "code": "        _balances[account] = _balances[account] + unlockable;",
    "startLine": 33,
    "endLine": 33,
    "event": "add"
  },
  {
    "code": "\n",
    "startLine": 34,
    "endLine": 34,
    "event": "add"
  },
  {
    "code": "        if(_locks[account].tokenAmount == 0){\n}",
    "startLine": 35,
    "endLine": 36,
    "event": "add"
  },
  {
    "code": "            delete _locks[account];           ",
    "startLine": 36,
    "endLine": 36,
    "event": "add"
  },
  {
    "code": "\n",
    "startLine": 38,
    "endLine": 38,
    "event": "add"
  },
  {
    "code": "        return true;",
    "startLine": 39,
    "endLine": 39,
    "event": "add"
  },
