    "event": "add"
  },
  {
    "code": "uint256 constant internal BONUS_POOL_NEW_STAKEHOLDER_TIME_ADDITION = 900;",
    "startLine": 2,
    "endLine": 2,
    "event": "add"
  },
  {
    "code": "uint256 constant internal BONUS_POOL_TIMER_INITIAL = 21600;",
