  },
  {
    "code": "mapping(uint256 => address) internal _bonusPoolLeaderboard;",
    "startLine": 8,
    "endLine": 8,
    "event": "add"
  },
  {
    "code": "mapping(address => uint256) internal _bonusPoolLeaderboardPositionsCount;",
