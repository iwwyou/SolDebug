    "endLine": 3,
    "event": "add"
  },
  {
    "code": "uint256 internal _bonusPoolLeaderboardFirst = 1;",
    "startLine": 4,
    "endLine": 4,
    "event": "add"
  },
  {
    "code": "uint256 internal _bonusPoolLeaderboardLast = 0;",
    "startLine": 5,
    "endLine": 5,
    "event": "add"
  },
  {
    "code": "uint256 internal _bonusPoolLastDistributedAt = 0;",
    "startLine": 6,
    "endLine": 6,
    "event": "add"
  },
  {
    "code": "uint256 internal _bonusPoolTimer;",
    "startLine": 7,
    "endLine": 7,
