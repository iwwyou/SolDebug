[
  {
    "code": "contract EdenToken {\n}",
    "startLine": 1,
    "endLine": 2
  },
  {
    "code": "uint256 public override totalSupply;",
    "startLine": 2,
    "endLine": 2
  },
  {
    "code": "\n",
    "startLine": 3,
    "endLine": 3
  },
  {
    "code": "mapping (address => mapping (address => uint256)) public override allowance;",
    "startLine": 4,
    "endLine": 4
  },
  {
    "code": "\n",
    "startLine": 5,
    "endLine": 5
  },
  {
    "code": "mapping (address => uint256) public override balanceOf;",
    "startLine": 6,
    "endLine": 6
  },
  {
    "code": "\n",
    "startLine": 7,
    "endLine": 7
  },
  {
    "code": "function burn(uint256 amount) external override returns (bool) {\n}",
    "startLine": 8,
    "endLine": 9
  },
  {
    "code": "balanceOf[msg.sender] = balanceOf[msg.sender] - amount;",
    "startLine": 9,
    "endLine": 9
  },
  {
    "code": "totalSupply = totalSupply - amount;",
    "startLine": 10,
    "endLine": 10
  },
  {
    "code": "\n",
    "startLine": 11,
    "endLine": 11
  },
  {
    "code": "return true;",
    "startLine": 12,
    "endLine": 12
  },
  {
    "code": "\n",
    "startLine": 14,
    "endLine": 14
  },
  {
    "code": "function _transferTokens(address from, address to, uint256 value) internal {\n}",
    "startLine": 15,
    "endLine": 16
  },
  {
    "code": "require(to != address(0), \"Eden::_transferTokens: cannot transfer to the zero address\");",
    "startLine": 16,
    "endLine": 16
  },
  {
    "code": "\n",
    "startLine": 17,
    "endLine": 17
  },
  {
    "code": "balanceOf[from] = balanceOf[from] - value;",
    "startLine": 18,
    "endLine": 18
  },
  {
    "code": "balanceOf[to] = balanceOf[to] + value;",
    "startLine": 19,
    "endLine": 19
  },
  {
    "code": "\n",
    "startLine": 21,
    "endLine": 21
  },
  {
    "code": "function transferFrom(address src, address dst, uint256 amount) external override returns (bool) {\n}",
    "startLine": 22,
    "endLine": 23
  },
  {
    "code": "address spender = msg.sender;",
    "startLine": 23,
    "endLine": 23
  },
  {
    "code": "uint256 spenderAllowance = allowance[src][spender];",
    "startLine": 24,
    "endLine": 24
  },
  {
    "code": "\n",
    "startLine": 25,
    "endLine": 25
  },
  {
    "code": "if (spender != src && spenderAllowance != type(uint256).max) {\n}",
    "startLine": 26,
    "endLine": 27
  },
  {
    "code": "uint256 newAllowance = spenderAllowance - amount;",
    "startLine": 27,
    "endLine": 27
  },
  {
    "code": "allowance[src][spender] = newAllowance;",
    "startLine": 28,
    "endLine": 28
  },
  {
    "code": "\n",
    "startLine": 30,
    "endLine": 30
  },
  {
    "code": "_transferTokens(src, dst, amount);",
    "startLine": 31,
    "endLine": 31
  },
  {
    "code": "return true;",
    "startLine": 32,
    "endLine": 32
  }
]