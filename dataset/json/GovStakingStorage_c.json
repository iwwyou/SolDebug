  {
    "code": "function getUserInfoByIndex(uint256 from, uint256 to) external view returns (UserInfo[] memory) {\n}",
    "startLine": 53,
    "endLine": 54,
    "event": "add"
  },
  {
    "code": "uint256 to_ = to > userList.length ? userList.length : to;",
    "startLine": 54,
    "endLine": 54,
    "event": "add"
  },
  {
    "code": "UserInfo[] memory result = new UserInfo[](to - from);",
    "startLine": 55,
    "endLine": 55,
    "event": "add"
  },
  {
    "code": "for (uint256 i = 0; i < to_ - from; i++) {\n}",
    "startLine": 56,
    "endLine": 57,
    "event": "add"
  },
  {
    "code": "result[i] = userInfo[userList[i + from]];",
    "startLine": 57,
