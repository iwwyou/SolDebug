  {
    "code": "contract LockupContract {\n}",
    "startLine": 1,
    "endLine": 2,
    "event": "add"
  },
  {
    "code": "    uint constant public SECONDS_IN_ONE_MONTH = 2628000;",
    "startLine": 2,
    "endLine": 2,
    "event": "add"
  },
  {
    "code": "    uint public immutable initialAmount; ",
    "startLine": 3,
    "endLine": 3,
    "event": "add"
  },
  {
    "code": "    uint public immutable deploymentStartTime;    ",
    "startLine": 4,
    "endLine": 4,
    "event": "add"
  },
  {
    "code": "    uint public immutable monthsToWaitBeforeUnlock;",
    "startLine": 5,
    "endLine": 5,
    "event": "add"
  },
  {
    "code": "    uint public immutable releaseSchedule;",
    "startLine": 6,
    "endLine": 6,
    "event": "add"
  },
  {
    "code": "\n",
    "startLine": 7,
    "endLine": 7,
    "event": "add"
  },
  {
    "code": "    function _getReleasedAmount() internal view returns (uint) {\n}",
    "startLine": 8,
    "endLine": 9,
    "event": "add"
  },
  {
    "code": "        uint unlockTimestamp = deploymentStartTime + (monthsToWaitBeforeUnlock * SECONDS_IN_ONE_MONTH);",
    "startLine": 9,
    "endLine": 9,
    "event": "add"
  },
  {
    "code": "        if (block.timestamp < unlockTimestamp) {\n}",
    "startLine": 10,
    "endLine": 11,
    "event": "add"
  },
  {
    "code": "            return 0;",
    "startLine": 11,
    "endLine": 11,
    "event": "add"
  },
  {
    "code": "        uint monthsSinceUnlock = ((block.timestamp - unlockTimestamp) / SECONDS_IN_ONE_MONTH) + 1;",
    "startLine": 13,
    "endLine": 13,
    "event": "add"
  },
  {
    "code": "        uint monthlyReleaseAmount = initialAmount / releaseSchedule;",
    "startLine": 14,
    "endLine": 14,
    "event": "add"
  },
  {
    "code": "        uint releasedAmount = monthlyReleaseAmount * monthsSinceUnlock;",
    "startLine": 15,
    "endLine": 15,
    "event": "add"
  },
  {
    "code": "\n",
    "startLine": 16,
    "endLine": 16,
    "event": "add"
  },
  {
    "code": "        if (releasedAmount > initialAmount){\n}",
    "startLine": 17,
    "endLine": 18,
    "event": "add"
  },
  {
    "code": "            return initialAmount;",
    "startLine": 18,
    "endLine": 18,
    "event": "add"
  },
  {
    "code": "\n",
    "startLine": 20,
    "endLine": 20,
    "event": "add"
  },
  {
    "code": "        return releasedAmount;",
    "startLine": 21,
    "endLine": 21,
    "event": "add"
  },
