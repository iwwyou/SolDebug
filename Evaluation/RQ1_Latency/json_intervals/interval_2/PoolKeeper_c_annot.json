[
  {
    "code": "contract PoolKeeper {\n}",
    "startLine": 1,
    "endLine": 2,
    "event": "add"
  },
  {
    "code": "    uint256 public constant BASE_TIP = 5; ",
    "startLine": 2,
    "endLine": 2,
    "event": "add"
  },
  {
    "code": "    uint256 public constant TIP_DELTA_PER_BLOCK = 5; ",
    "startLine": 3,
    "endLine": 3,
    "event": "add"
  },
  {
    "code": "    uint256 public constant BLOCK_TIME = 13;  ",
    "startLine": 4,
    "endLine": 4,
    "event": "add"
  },
  {
    "code": "    uint256 public constant MAX_TIP = 100; ",
    "startLine": 5,
    "endLine": 5,
    "event": "add"
  },
  {
    "code": "\n",
    "startLine": 6,
    "endLine": 6,
    "event": "add"
  },
  {
    "code": "    function keeperTip(uint256 _savedPreviousUpdatedTimestamp, uint256 _poolInterval) public view returns (uint256) {        \n}",
    "startLine": 7,
    "endLine": 8,
    "event": "add"
  },
  {
    "code": "        uint256 elapsedBlocksNumerator = (block.timestamp - (_savedPreviousUpdatedTimestamp + _poolInterval));",
    "startLine": 8,
    "endLine": 8,
    "event": "add"
  },
  {
    "code": "\n",
    "startLine": 9,
    "endLine": 9,
    "event": "add"
  },
  {
    "code": "        uint256 keeperTip = BASE_TIP + (TIP_DELTA_PER_BLOCK * elapsedBlocksNumerator) / BLOCK_TIME;",
    "startLine": 10,
    "endLine": 10,
    "event": "add"
  },
  {
    "code": "\n",
    "startLine": 11,
    "endLine": 11,
    "event": "add"
  },
  {
    "code": "        if (keeperTip > MAX_TIP) {\n}",
    "startLine": 12,
    "endLine": 13,
    "event": "add"
  },
  {
    "code": "            return MAX_TIP;",
    "startLine": 13,
    "endLine": 13,
    "event": "add"
  },
  {
    "code": "else {\n}",
    "startLine": 14,
    "endLine": 15,
    "event": "add"
  },
  {
    "code": "            return keeperTip;",
    "startLine": 15,
    "endLine": 15,
    "event": "add"
  },
  {
